{
	"appId": "com.mirro2.app",
	"appName": "MirroSocial",
	"webDir": "out",
	"server": {
		"androidScheme": "https",
		"url": "https://www.mirro2.com",
		"cleartext": false
	},
	"android": {},
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 1500,
			"backgroundColor": "#FFFFFF",
			"showSpinner": false,
			"androidSpinnerStyle": "large",
			"spinnerColor": "#999999",
			"androidScaleType": "CENTER_CROP",
			"splashFullScreen": true,
			"splashImmersive": true
		},
		"App": {
			"initialPath": "/login"
		}
	},
	"packageClassList": [
		"AppPlugin",
		"CAPCameraPlugin",
		"DevicePlugin",
		"FilesystemPlugin",
		"HapticsPlugin",
		"KeyboardPlugin",
		"CAPNetworkPlugin",
		"PreferencesPlugin",
		"SharePlugin",
		"SplashScreenPlugin",
		"StatusBarPlugin"
	]
}
